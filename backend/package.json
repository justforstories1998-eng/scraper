{
  "name": "webmethods-scraper-backend",
  "version": "1.0.0",
  "description": "Backend server for webMethods web scraping application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "scrape": "node -e \"require('./scrapers/scraperManager').runAllScrapers()\"",
    "scrape:news": "node -e \"require('./scrapers/newsScraper').scrape()\"",
    "scrape:jobs": "node -e \"require('./scrapers/jobScraper').scrape()\"",
    "scrape:blogs": "node -e \"require('./scrapers/blogScraper').scrape()\""
  },
  "keywords": [
    "webmethods",
    "scraper",
    "web-scraping",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "mongoose": "^8.21.0",
    "node-cron": "^3.0.3",
    "puppeteer": "^24.34.0",
    "robots-parser": "^3.0.1",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.11"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "commonjs"
}
